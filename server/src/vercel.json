{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node",
        "config": {
            "includeFiles": ["src/**/*"],
            "distDir": "dist"
          }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/server.ts"
      }
    ],
    "env": {
      "MONGO_PASSWORD": "@MONGO_PASSWORD"
    }
  }
  